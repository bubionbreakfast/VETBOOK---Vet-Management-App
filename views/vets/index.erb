<h1>Vets</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <% for vet in @vets %>
  <tr>
    <td><a href="/vets/<%= vet.id %>"><%= vet.first_name %></a></td>
    <td>|</td>
    <td><a href="/vets/<%= vet.id %>"><%= vet.last_name %></a></td>

    <!-- <td><%= vet.first_name %></td>
    <td><%= vet.last_name %></td> -->
  </tr>
  <% end %>
</table>
