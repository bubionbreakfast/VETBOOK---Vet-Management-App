<h1>Vets</h1>
<p><a href="/vets/new">Create Vet</a>
</p>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Vet ID Number</th>
  </tr>
  <% for vet in @vets %>
  <tr>
    <td><a href="/vets/<%= vet.id %>"><%= vet.first_name %></a></td>
    <td>|</td>
    <td><a href="/vets/<%= vet.id %>"><%= vet.last_name %></a></td>
    <td>|</td>
    <td><a href="/vets/<%= vet.id %>"><%= vet.id %></a></td>
  
  </tr>
  <% end %>
</table>
