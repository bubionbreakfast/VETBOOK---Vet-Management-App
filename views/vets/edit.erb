<form method="post" action="/vets/<%= @vets.id %>">
  <label for="first-name-field">First Name:</label>
  <input
  type="text"
  name="first_name"
  id="first-name-field"
  value="<%= @vets.first_name %>"
  />

  <label for="last-name-field">Last Name:</label>
  <input
  type="text"
  name="last_name"
  id="last-name-field"
  value="<%= @vets.last_name %>"
  />

  <input class="btn btn--action" type='submit' value="Save Vet">
</form>
<input class="btn btn--danger" type="submit" value="Delete Vet"
